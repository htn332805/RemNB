                        <outline text="Distinguishing">
                            <outline text="Perfect Security Markets">
                                <outline text="Complete information readily available"/>
                                <outline text="No transaction costs"/>
                                <outline text="Perfect liquidity"/>
                                <outline text="No barriers to entry or exit"/>
                                <outline text="Homogeneous expectations"/>
                                <outline text="Rational investors"/>
                            </outline>
                            <outline text="Imperfect Markets">
                                <outline text="Create a Need for Financial Intermediaries">
                                    <outline text="bridge the information gap between buyers and sellers of securities">
                                        <outline text="reducing information asymmetry in the market"/>
                                        <outline text="specialized knowledge and resources"/>
                                        <outline text="analyze, and disseminate information"/>
                                    </outline>
                                    <outline text="reduce transaction costs">
                                        <outline text="aggregating trades and providing economies of scale"/>
                                        <outline text="cost-effective for individual investors"/>
                                    </outline>
                                    <outline text="Liquidity provision">
                                        <outline text="improve market efficiency"/>
                                        <outline text="educes the impact of large trades on prices"/>
                                    </outline>
                                    <outline text="Risk management">
                                        <outline text="help manage and distribute risk more effectively">
                                            <outline text="across the financial system."/>
                                        </outline>
                                        <outline text="diversified portfolios"/>
                                        <outline text="offer risk management products"/>
                                    </outline>
