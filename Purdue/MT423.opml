                                <outline text="selecting securities randomly"/>
                                <outline text="risk declines rapidly initially"/>
                                <outline text="benefits kick in immediately"/>
                                <outline text="Cannot completely eliminate risk">
                                    <outline text="Market risk remains"/>
                                </outline>
                                <outline text="marginal risk reduction decrease with more securities"/>
                                <outline text="Generally not optimal"/>
                            </outline>
                            <outline text="efficient diversification">
