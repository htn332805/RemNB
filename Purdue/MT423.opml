                        <outline text="components of portfolio risk">
                            <outline text="Weight individual security risk">
                                <outline text="variance"/>
                            </outline>
                            <outline text="weight comovements btw security returns">
                                <outline text="Covariance"/>
                            </outline>
                        </outline>
                        <outline text="covariance">
                            <outline text="Extent two random variables covary"/>
                            <outline text="positive: returns move in same direction"/>
                            <outline text="negative: returns move inversely"/>
                            <outline text="Zero: return independent"/>
                            <outline text="size depends on units"/>
                            <outline text="primarily indicates direction (+/-/0)"/>
                            <outline text="limited guidance on strength of association"/>
                        </outline>
                        <outline text="correlation coefficient">
                            <outline text="Statistical measure of relative comovements"/>
                            <outline text="Measures extent returns move together"/>
                            <outline text="denotes association (not causation)"/>
                            <outline text="standardized covariance measure"/>
                            <outline text="bounded by +1 and -1"/>
                            <outline text="+1: perfect postive correlation"/>
                            <outline text="-1: perfect negative correlation"/>
                            <outline text="0: Zero, or No, Correlation"/>
                            <outline text="indicates direction and strength"/>
                            <outline text="More popular measure of comovement"/>
                        </outline>
                        <outline text="impact of correlation of risk reduction">
                            <outline text="Perfect positive correlation( +1): No risk reduction"/>
                            <outline text="Perfect negative correlation(-1): risk can be complete eliminate"/>
                            <outline text="less than perfect positive correlation: risk reduction occurs"/>
                            <outline text="lower positive correlation=better risk reduction"/>
                        </outline>
                        <outline text="the n-security case">
                            <outline text="Portfolio risk=f(variances,covariances,weights)"/>
                            <outline text="adding security to large portfolio"/>
                            <outline text="asset's own risk added"/>
                            <outline text="Weighted covariances added"/>
                            <outline text="covariance becomes more important than variance"/>
                            <outline text="individual risks diversified away"/>
                            <outline text="covariance terms contribute to portfolio risk"/>
                        </outline>
                        <outline text="simplifying Markowitz Calculations">
                            <outline text="requires full set of security return covariances"/>
                            <outline text="Number of covariances=n(n-1)"/>
                            <outline text="Number of covariances=[n(n-1)]/2"/>
                            <outline text="large number of inputs for many securities"/>
                            <outline text="simplification needed for practical user"/>
                            <outline text="relation of each security to an index (suggested by Markowitz)"/>
                        </outline>
