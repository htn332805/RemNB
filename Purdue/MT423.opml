                        <outline text="nonsystematic risk">
                            <outline text="Diversifiable risk"/>
                            <outline text="company specific risk"/>
                            <outline text="can be eliminated by diversification"/>
                        </outline>
                        <outline text="systematic risk">
                            <outline text="nondiversifiable risk"/>
                            <outline text="market risk"/>
                            <outline text="associated with overall market/economy movements"/>
                            <outline text="cannot be eliminated by diversification"/>
                            <outline text="critical to all investors"/>
                            <outline text="Relevant risk for assets pricing"/>
                        </outline>
                    </outline>
